<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RentACar"
        tools:targetApi="31">
        <activity
            android:name=".MultiStepCheckoutActivity"
            android:exported="false" />
        <activity
            android:name=".FactureActivity"
            android:exported="false" />
        <activity
            android:name=".PaymentFailedActivity"
            android:exported="false" />
        <activity
            android:name=".PaymentConfirmationActivity"
            android:exported="false" />
        <activity
            android:name=".PaymentMethodActivity"
            android:exported="false" />
        <activity
            android:name=".CheckoutInfoActivity"
            android:exported="false" />
        <activity
            android:name=".InvoiceTransactionHistoryActivity"
            android:exported="false" />
        <activity
            android:name=".PaymentDetailPayPalActivity"
            android:exported="false" />
        <activity
            android:name=".PaymentDetailVisaActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>